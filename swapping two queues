#include <bits/stdc++.h>
using namespace std;
int main()
{
    int a[5]={8,2,3,4,5};
    queue<int> q;
    queue<int> m;
    int b[7]={1,2,3,4,5,6,7};
    for(int i=0;i<5;i++)
    {
        q.push(a[i]);
    }
    for(int i=0;i<7;i++)
    {
        m.push(b[i]);
    }
    q.swap(m);
    while(!q.empty())
    {
        cout<<q.front()<<" ";
        q.pop();
    }
    
}
